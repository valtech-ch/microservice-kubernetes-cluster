{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve --port 3000",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build",
    "lint": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service lint",
    "sonar": "node sonar-project.js",
    "test": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "@opentelemetry/context-zone": "1.5.0",
    "@opentelemetry/core": "1.4.0",
    "@opentelemetry/exporter-zipkin": "1.4.0",
    "@opentelemetry/instrumentation-document-load": "0.29.0",
    "@opentelemetry/instrumentation-xml-http-request": "0.30.0",
    "@opentelemetry/resources": "1.4.0",
    "@opentelemetry/sdk-trace-base": "1.4.0",
    "@opentelemetry/sdk-trace-web": "1.4.0",
    "@opentelemetry/semantic-conventions": "1.4.0",
    "axios": "0.27.2",
    "core-js": "3.23.4",
    "keycloak-js": "18.0.1",
    "postcss": "8.4.14",
    "vue": "3.2.37",
    "vue-axios": "3.4.1",
    "vue-class-component": "8.0.0-rc.1",
    "vue-cookie-next": "1.3.0",
    "vue-router": "4.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-router": "5.0.4",
    "@vue/cli-plugin-typescript": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/compiler-sfc": "3.2.37",
    "@vue/eslint-config-typescript": "11.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.19.0",
    "eslint-plugin-vue": "9.2.0",
    "jest": "28.1.2",
    "jest-sonar-reporter": "2.0.0",
    "sonarqube-scanner": "2.8.1",
    "supertest": "6.2.4",
    "typescript": "4.7.4",
    "vue-cli-plugin-compression": "1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
